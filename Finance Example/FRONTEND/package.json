{
  "name": "react-redux-antdesign-webpack-starter",
  "version": "1.0.1",
  "description": "react js + redux + ant design + react router + hot reload + devTools extension + webpack starter",
  "main": "src/index.js",
  "scripts": {
    "flow": "node_modules/.bin/flow",
    "test": "jest && codecov",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "rimraf docs/public/assets/ && cross-env NODE_ENV=dev webpack --config webpack.dev.config.js",
    "prod": "rimraf docs/public/assets/ && cross-env NODE_ENV=production webpack -p --config webpack.production.config.js ",
    "start": "cross-env NODE_ENV=dev node server.hot.reload",
    "serve-dev": "npm run dev && cross-env NODE_ENV=dev nodemon ./src/server/index.js",
    "serve-prod": "npm run prod && cross-env NODE_ENV=production node ./src/server/index.js"
  },
  "engines": {
    "node": ">=8.1.4",
    "npm": ">=5.3.0",
    "npx": ">=9.2.0",
    "yarn": ">=0.27.5"
  },
  "keywords": [
    "react",
    "redux",
    "antd",
    "ant",
    "design",
    "react-redux",
    "ES6",
    "ES2015",
    "react-router4",
    "react-router",
    "starter",
    "webpack",
    "hot-reload",
    "redux-devtools-extension",
    "devtools",
    "webpack3"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MacKentoch/react-redux-antdesign-webpack-starter.git"
  },
  "bugs": {
    "url": "https://github.com/MacKentoch/react-redux-antdesign-webpack-starter/issues"
  },
  "author": "Erwan DATIN (MacKentoch)",
  "license": "MIT",
  "devDependencies": {
    "@types/antd": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/chalk": "^0.4.31",
    "@types/classnames": "^2.2.3",
